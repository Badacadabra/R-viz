{
  "name": "R-viz",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint",
    "build:linux": "electron-packager ./electron R-viz --overwrite --asar --platform=linux --arch=x64 --icon=resources/icon.png --prune=true --out=release-builds && electron-installer-debian --src release-builds/R-viz-linux-x64/ --arch amd64 --config debian.json",
